{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "wwwroot",
    "clearUrls": true,
    "headers": [
      {
        "source": "*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM *"
          }
        ]
      }
    ]
  }
}
